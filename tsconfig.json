{
  "include": ["**/*"],
  "exclude": [],
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "target": "es2016",
    "module": "es2015",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "noEmit": true
  }
}
